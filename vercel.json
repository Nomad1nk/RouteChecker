{
  "version": 2,
  "builds": [
    {
      "src": "frontend-dashboard/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "optimization-engine/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/routes/optimize",
      "dest": "/optimization-engine/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-dashboard/$1"
    }
  ]
}
